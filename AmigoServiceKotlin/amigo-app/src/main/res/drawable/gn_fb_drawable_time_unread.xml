<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size android:width="20dp" android:height="20dp"/>
            <solid android:color="@color/gn_fb_color_time_solid"/>
        </shape>
    </item>
    <item android:top="3dp"
          android:bottom="3dp"
          android:left="3dp"
          android:right="3dp">
        <shape android:shape="oval">
            <solid android:color="@color/gn_fb_color_orange"/>
        </shape>
    </item>
    <item android:top="4dp"
          android:bottom="4dp"
          android:left="4dp"
          android:right="4dp">
        <shape android:shape="oval">
            <solid android:color="@color/gn_fb_color_time_solid"/>
        </shape>
    </item>
    <item android:top="6dp"
          android:bottom="6dp"
          android:left="6dp"
          android:right="6dp">
        <shape android:shape="oval">
            <solid android:color="@color/gn_fb_color_orange"/>
        </shape>
    </item>
</layer-list>