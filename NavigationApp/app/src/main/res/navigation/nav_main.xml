<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.lee.navigation.fragment.HomeFragment"
        android:label="home"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_homeFragment_to_videoFragment2"
            app:destination="@id/videoFragment" />
        <action
            android:id="@+id/action_homeFragment_to_meFragment3"
            app:destination="@id/meFragment" />
    </fragment>

    <fragment
        android:id="@+id/videoFragment"
        android:name="com.lee.navigation.fragment.VideoFragment"
        android:label="video"
        tools:layout="@layout/fragment_video">

        <action
            android:id="@+id/action_videoFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_videoFragment_to_meFragment"
            app:destination="@id/meFragment" />
    </fragment>


    <fragment
        android:id="@+id/meFragment"
        android:name="com.lee.navigation.fragment.MeFragment"
        android:label="me"
        tools:layout="@layout/fragment_me">

        <action
            android:id="@+id/action_meFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_meFragment_to_videoFragment"
            app:destination="@id/videoFragment" />
    </fragment>


</navigation>